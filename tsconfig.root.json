{
  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@backend-template/prisma": ["packages/prisma/src"]
    },
    "types": ["node"]
  },
  "exclude": ["node_modules", "dist"]
}
